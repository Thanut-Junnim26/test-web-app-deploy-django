{
    "builds": [{
        "src": "medicine_store/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles_build/$1"
        },
        {
            "src": "/(.*)",
            "dest": "medicine_store/wsgi.py"
        }
    ]
}
